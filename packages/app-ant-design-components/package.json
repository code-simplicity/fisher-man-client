{
  "name": "app-ant-design-components",
  "version": "0.0.1",
  "description": "基于antd5二次开发的摸鱼君组件库",
  "keywords": [
    "ant-design",
    "antd",
    "compoents",
    "react",
    "dumi"
  ],
  "bugs": {
    "url": "https://github.com/dpy0912/fisher-man-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dpy0912/fisher-man-client/tree/develop/packages/app-ant-design-components"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "unpkg": "dist/antd.min.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "build:APP-AND-DESIGN-COMPONENTS": "father build --clean=true",
    "build:watch": "father dev",
    "dev:APP-AND-DESIGN-COMPONENTS": "dumi dev --watch",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start:APP-AND-DESIGN-COMPONENTS": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix"
    ],
    "*.{js,jsx}": [
      "eslint --fix-dry-run"
    ],
    "*.{ts,tsx}": [
      "eslint --fix-dry-run"
    ]
  },
  "dependencies": {
    "antd-img-crop": "^4.5.2",
    "classnames": "^2.3.2",
    "deep-equal": "^2.2.0",
    "lodash": "^4.17.21",
    "omit.js": "^2.0.2",
    "rc-util": "^5.27.1",
    "react-draggable": "^4.4.5",
    "react-viewer": "^3.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@icon-park/react": "^1.4.2",
    "@types/deep-equal": "^1.0.1",
    "@types/lodash": "^4.14.191",
    "@types/uuid": "^9.0.0",
    "@umijs/lint": "^4.0.0",
    "ahooks": "^3.7.2",
    "antd": "^5.0.7",
    "dumi": "^2.0.2",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "dupyi0912@gmail.com"
  ],
  "title": "app-ant-design-components"
}
